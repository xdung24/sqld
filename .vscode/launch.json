  "version": "0.2.0",
  "configurations": [
    {
      "name": "Attach to Process",
      "type": "go",
      "request": "attach",
      "mode": "local",
      "processId": 0
    },
    {
      "name": "Launch Package",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceRoot}",
      "args": [
        "-u",
        "root",
        "-h",
        "localhost:3306",
        "-type",
        "mysql",
        "-db",
        "divawallet",
        "-p",
        "divawallet",
        "-raw",
        "-url",
        "/",
        "--port",
        "8888"
      ]
    }
  ]
}
